{
  "name": "kolour",
  "version": "1.0.0",
  "description": "A fluent color conversion and manipulation library",
  "main": "index.js",
  "scripts": {
    "clean": "node_modules/.bin/rimraf build/*",
    "build:babel": "node_modules/.bin/babel src/**/*.es6 --source-maps --out-dir build",
    "build:webpack": "node_modules/.bin/webpack",
    "test": "node_modules/.bin/mocha",
    "test:karma": "node_modules/.bin/karma start karma.config.js",
    "test:mocha": "node_modules/.bin/mocha",
    "coverage": "node_modules/.bin/nyc --require babel-core/register --extension .es6 --report-dir build/coverage --reporter html --reporter lcov npm run test",
    "document": "node_modules/.bin/jsdoc -c jsdoc.config.json src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/t28hub/kolour.git"
  },
  "keywords": [
    "color",
    "color-space",
    "parser",
    "converter"
  ],
  "author": {
    "name": "Tatsuya Maki",
    "email": "t28oogle@gmail.com",
    "url": "http://t28.io/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/t28hub/kolour/issues"
  },
  "homepage": "https://github.com/t28hub/kolour#readme",
  "dependencies": {
    "babel-polyfill": "^6.7.4"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-plugin-espower": "^2.1.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "jsdoc": "^3.4.0",
    "json-loader": "^0.5.4",
    "karma": "^0.13.22",
    "karma-espower-preprocessor": "0.0.4",
    "karma-mocha": "^0.2.2",
    "karma-mocha-reporter": "^2.0.1",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.5",
    "nyc": "^6.4.0",
    "phantomjs-prebuilt": "^2.1.4",
    "power-assert": "^1.3.1",
    "rimraf": "^2.5.2",
    "webpack": "^1.13.0"
  },
  "engines": {
    "node": "~5.10.0"
  }
}
